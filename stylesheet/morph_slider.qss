QSlider{
    min-height: 60px;
    min-width: 60px;
}
QSlider::groove:horizontal {
    border: 1px solid #ababab;
    border-radius: 4px;
    height: 9px;
    background: #d9d9d9;
}
QSlider::handle:horizontal{
    border-image:url(slider_image_path);
    width:  55px;
    height: 55px;
    margin-top: -25px;
    margin-left: -2px;
    margin-bottom: -25px;
    margin-right: -1px;
}
